<!-- NavBar -->





<h1>Search Results</h1>


<% @result.each do |result| %>

 <ul>
  <li> <%= result.screen_name %> </li>

   <%= image_tag(result.profile_image_url , size: "50x50", :alt => "rss feed", :class => "img-circle") %>
  <li> <%= result.name %> </li>
  <li> <%= result.text %> </li>
  <li> <%= result.created_at %> </li>

    <% if result.likes.size > 0 %>
      <%= link_to 'unLike', rawresult_like_path(result, current_user), method: :delete %>
     <% else  %>
      <%= link_to 'Like', rawresult_likes_path(result), method: :post %>
    <% end %>

</ul>

<% end %>
